 A simple Makefile

#target -> dependencies
#       -> action


# Makefile commands

all: enterPwd

enterPwd: enterPwd.o
        gcc enterPwd.o -o enterPwd

# all  *.c files are dependencies

enterPwd.0: enterPwd.c
        gcc -C enterPwd.c # compile only

# clean all object files and the executables
clean:
        rm *.o enterPwd


